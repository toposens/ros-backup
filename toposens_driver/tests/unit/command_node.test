<launch>
  <arg name="port" default="/dev/tnt0" />
  <arg name="frame" default="toposens" />
  <arg name="t_celsius" default="24.5" />

  <!-- Command Test -->
  <!-- Tests generation of parameter setting commands. -->
  <test pkg="toposens_driver"
        type="toposens_driver_command_test"
        test-name="toposens_driver_command_test"
        time-limit="5.0">
    <param name="port" value="$(arg port)" />
    <param name="frame" value="$(arg frame)" />
    <param name="t_celsius" value="$(arg t_celsius)" />
  </test>
</launch>