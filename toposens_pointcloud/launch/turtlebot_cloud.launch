<launch>
	<include file="$(find turtlebot3_bringup)/launch/turtlebot3_remote.launch"/>

	<include file="$(find turtlebot3_teleop)/launch/turtlebot3_teleop_key.launch"/>

	<node pkg="toposens_pointcloud" type="toposens_pointcloud_node" name="ts_cloud_node" output="screen">
		<param name="fixed_frame" value="odom" />
	</node>

	<node pkg="rviz" type="rviz" name="grid_visualization" output="screen" args="-d $(find toposens_pointcloud)/rviz/toposens_turtlebot.rviz" />

</launch>
